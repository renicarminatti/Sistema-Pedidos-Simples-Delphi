  object qryListProduto: TFDQuery
    Connection = DM.conMySQL
    SQL.Strings = (
      'SELECT'
      '  i.NUMEROITEM,'
      '  i.NUMEROPEDIDO,'
      #10'  i.CODIGOPRODUTO,'
      #10'  p.DESCRICAO,'
      #10'  i.QUANTIDADE,'
      #10'  i.VALORUNITARIO,'
      #10'  i.VALORTOTALITEM'
      #10'FROM itens AS i'
      #10'INNER JOIN produtos AS P'
      #10'ON (i.CODIGOPRODUTO=P.CODIGO)'
      #10'where'
      '  i.NUMEROPEDIDO = :NUMEROPEDIDO'
      ''
      ''
      '')
    Left = 472
    Top = 248
    ParamData = <
      item
        Name = 'NUMEROPEDIDO'
        ParamType = ptInput
      end>
